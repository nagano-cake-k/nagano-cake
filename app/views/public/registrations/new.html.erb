<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "public/shared/error_messages", resource: resource %>

  <div class="field">
    <tr>
      <td>
        <%= f.label :（姓） %>
        <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
      </td>
       <td>
        <%= f.label :（名） %>
        <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
      </td>
    </tr>
  </div>
  
  
  
  <div class="field">
    <tr>
      <td>
        <%= f.label :（セイ） %>
        <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana" %>
      </td>
      <td>
        <%= f.label :（メイ） %>
        <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana" %>
      </td>
    </tr>
  </div>
  
   <div class="field">
    <%= f.label :メールアドレス %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>
  
  
  <div class="郵便番号">
    <%= f.label :post_code %><br />
    <%= f.text_field :email, autofocus: true, autocomplete: "post_code" %>
  </div>
  
  <div class="住所">
    <%= f.label :address %><br />
    <%= f.text_field :email, autofocus: true, autocomplete: "address" %>
  </div>

  <div class="電話番号">
    <%= f.label :telephone_number %><br />
    <%= f.telephone_field :email, autofocus: true, autocomplete: "email" %>
  </div>telephone_number

  
 
  

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "public/shared/links" %>
